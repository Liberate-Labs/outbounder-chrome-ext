{
  "manifest_version": 3,
  "version": "1.0",
  "name": "Liberate Scrapper",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "128": "logo.png"
    }
  },
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"]
    }
  ]
}
